{
  "kind": "collectionType",
  "collectionName": "product_reviews",
  "info": {
    "singularName": "product-review",
    "pluralName": "product-reviews",
    "displayName": "ProductReview"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "rating": {
      "type": "integer"
    },
    "approved": {
      "type": "boolean",
      "default": false
    },
    "comment": {
      "type": "string"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_reviews"
    }
  }
}
